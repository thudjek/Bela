{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "sweetalert2@9.17.1",
      "destination": "wwwroot/lib/sweetalert2/",
      "files": [
        "dist/sweetalert2.min.css",
        "dist/sweetalert2.min.js"
      ]
    },
    {
      "library": "@microsoft/signalr@3.1.7",
      "destination": "wwwroot/lib/signalr/",
      "files": [
